<table class="t1">
    <tr>
        <td class="t2">
            <span class="title1">Method</span>
        </td>
        <td>
            <span class="title1">Number</span>
        </td>
        <td>
            <span class="title1">Percent</span>
        </td>
        <td>
            <span class="title1">Completed Follow-up</span>
        </td>
        <td>
            <span class="title1">Percent Completed Follow-up</span>
        </td>
    </tr>
    <% @methods.each_with_index do |method, i| %>
        <% if i != 0 && !(Prcmethod.find_by_prcMethod_id i).nil? %>
            <tr class="<%= cycle('highlight', 'nohighlight') %>">
                <td>
                    <%= (Prcmethod.find_by_prcMethod_id i).prcMethod_desc %>
                </td>
                <td>
                    <center>
                        <%= method %>
                    </center>
                </td>
                <td>
                    <center>
                        <% if @total != 0 %>
                            <%= (method*100)/@total %>%
                        <% else %>
                            0%
                        <% end %>
                    </center>
                </td>
                <td>
                    <center>
                        <%= @completed[i] %>
                    </center>
                </td>
                <td>
                    <center>
                        <% if method != 0 %>
                            <%= (@completed[i]*100)/method %>%
                        <% else %>
                            0%
                        <% end %>
                    </center>
                </td>
            </tr>
        <% end %>
    <% end %>
</table>
