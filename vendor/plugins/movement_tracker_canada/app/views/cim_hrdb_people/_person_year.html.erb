<br/>
<% if @cim_hrdb_person_years and @cim_hrdb_person_years.size > 0 %>
  <table>
    <tr>
      <th>Description</th>
      <th>Grad date</th>
    </tr>

  <% @cim_hrdb_person_years.each do |cim_hrdb_person_year| %>
    <tr>
      <td><%=h cim_hrdb_person_year.school_year.name %></td>
      <td><%=h cim_hrdb_person_year.grad_date %></td>
      <td><%= link_to 'Edit', edit_cim_hrdb_person_cim_hrdb_person_year_path(@cim_hrdb_person, cim_hrdb_person_year) %></td>
      <td><%= link_to 'Destroy', cim_hrdb_person_cim_hrdb_person_year_path(@cim_hrdb_person, cim_hrdb_person_year), :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
  </table>
<% else %>
  <br/>This person has no year in school.<br/>
<% end %>
<br/>
<%= link_to 'New year in school', new_cim_hrdb_person_cim_hrdb_person_year_path(@cim_hrdb_person) %>
<br/>
