<h1>New cim_hrdb_person</h1>
<br/>
<% form_for(@cim_hrdb_person) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :fname %><br />
    <%= f.text_field :fname %>
  </p>
  <p>
    <%= f.label :lname %><br />
    <%= f.text_field :lname %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
<br/>
<%= link_to 'Back', cim_hrdb_people_path %>