<h1>New Country</h1>
<br/>
<% form_for(@country, :url => cim_hrdb_countries_path, :action => 'create') do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :desc, "Description" %><br />
    <%= f.text_field :desc %>
  </p>
  <p>
    <%= f.label :short_desc, "Short description" %><br />
    <%= f.text_field :short_desc %>
  </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
<br/>
<%= link_to 'Back', cim_hrdb_countries_path %>