<h1>Editing user</h1>

<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :guid %><br />
    <%= f.text_field :guid %>
  </p>
  <p>
    <%= f.label :accountgroup_id %><br />
    <%= f.text_field :accountgroup_id %>
  </p>
  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :language_id %><br />
    <%= f.text_field :language_id %>
  </p>
  <p>
    <%= f.label :viewer_isActive %><br />
    <%= f.check_box :viewer_isActive %>
  </p>
  <p>
    <%= f.label :last_login %><br />
    <%= f.date_select :last_login %>
  </p>
  <p>
    <%= f.label :remember_token %><br />
    <%= f.text_field :remember_token %>
  </p>
  <p>
    <%= f.label :remember_token_expires_at %><br />
    <%= f.datetime_select :remember_token_expires_at %>
  </p>
  <p>
    <%= f.label :email_validated %><br />
    <%= f.check_box :email_validated %>
  </p>
  <p>
    <%= f.label :developer %><br />
    <%= f.check_box :developer %>
  </p>
  <p>
    <%= f.label :facebook_hash %><br />
    <%= f.text_field :facebook_hash %>
  </p>
  <p>
    <%= f.label :facebook_username %><br />
    <%= f.text_field :facebook_username %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', accountadmin_user_path(@user) %> |
<%= link_to 'Back', accountadmin_users_path %>