
<% summer_report.summer_report_reviewers.each do |r| %>
  <p>
    <% if r.reviewed %>
      <b><%= "#{r.person.full_name} " %></b>reviewed this schedule and noted <i>"<%= r.review_notes %>"</i>
    <% else %>
      <b><%= "#{r.person.full_name} " %></b>hasn't reviewed this schedule
    <% end %>
  </p>
<% end %>