<h1>Listing Years in School</h1>
<br/>
<table>
  <tr>
    <th>Description</th>
  </tr>

<% @year_in_schools.each do |year_in_school| %>
  <tr>
    <td><%=h year_in_school.desc %></td>
    <td><%= link_to 'Show', year_in_school %></td>
    <td><%= link_to 'Edit', edit_year_in_school_path(year_in_school) %></td>
    <td><%= link_to 'Destroy', year_in_school, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New year in school', new_year_in_school_path %>