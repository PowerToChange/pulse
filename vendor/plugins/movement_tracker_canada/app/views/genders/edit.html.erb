<h1>Editing Gender</h1>
<br/>
<% form_for(@gender) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :desc, "Description" %><br />
    <%= f.text_field :desc %>
  </p>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>
<br/>
<%= link_to 'Show', @gender %> |
<%= link_to 'Back', genders_path %>