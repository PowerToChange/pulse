<h1>Editing Account Group</h1>
<br/>
<% form_for(@accountadmin_accountgroup) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :key %><br />
    <%= f.text_field :key %>
  </p>
  <p>
    <%= f.label :label_long %><br />
    <%= f.text_field :label_long %>
  </p>
  <p>
    <%= f.label :english_value %><br />
    <%= f.text_area :english_value %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @accountadmin_accountgroup %> |
<%= link_to 'Back', accountadmin_accountgroups_path %>