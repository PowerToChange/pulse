<h1>Listing Account Groups</h1>
<br/>
<table class="directorytable">
  <thead>
    <tr>
      <th>Key</th>
      <th>Label long</th>
      <th>English value</th>
    </tr>
  </thead>
  <tbody>
  <% @accountadmin_accountgroups.each do |accountadmin_accountgroup| %>
    <tr class="<%= cycle('highlight', 'nohighlight') %>">
      <td class="person_row" person_link=<%= accountadmin_accountgroup_path(accountadmin_accountgroup) %>><%=h accountadmin_accountgroup.key %></td>
      <td class="person_row" person_link=<%= accountadmin_accountgroup_path(accountadmin_accountgroup) %>><%=h accountadmin_accountgroup.label_long %></td>
      <td class="person_row" person_link=<%= accountadmin_accountgroup_path(accountadmin_accountgroup) %>><%=h accountadmin_accountgroup.english_value %></td>
      <td><%= link_to 'Show', accountadmin_accountgroup %></td>
      <td><%= link_to 'Edit', edit_accountadmin_accountgroup_path(accountadmin_accountgroup) %></td>
      <td><%= link_to 'Destroy', accountadmin_accountgroup, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New account group', new_accountadmin_accountgroup_path %>