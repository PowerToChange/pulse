<h1>Editing Access Category</h1>
<br/>
<% form_for(@accountadmin_accesscategory) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :key %><br />
    <%= f.text_field :key %>
  </p>
  <p>
    <%= f.label :english_value %><br />
    <%= f.text_area :english_value %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @accountadmin_accesscategory %> |
<%= link_to 'Back', accountadmin_accesscategories_path %>