<h1>Listing profile_pictures</h1>

<table>
  <tr>
  </tr>

<% for profile_picture in @profile_pictures %>
  <tr>
    <td><%= link_to 'Show', profile_picture %></td>
    <td><%= link_to 'Edit', edit_profile_picture_path(profile_picture) %></td>
    <td><%= link_to 'Destroy', profile_picture, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

