<h1>Editing Event Group</h1>
<br/>
<% form_for(@event_group) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>
<br/>
<%= link_to 'Show', @event_group %> |
<%= link_to 'Back', event_groups_path %>
